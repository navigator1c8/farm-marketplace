{
  "name": "farm-marketplace-backend",
  "version": "2.0.0",
  "description": "Расширенный Backend API для фермерского маркетплейса",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "node src/scripts/seedDatabase.js",
    "seed:clear": "node src/scripts/clearDatabase.js",
    "seed:dev": "node src/scripts/seedDevelopmentData.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "build": "echo 'No build step required for Node.js'",
    "logs:access": "tail -f logs/access.log",
    "logs:error": "tail -f logs/error.log",
    "backup:db": "node src/scripts/backupDatabase.js",
    "restore:db": "node src/scripts/restoreDatabase.js",
    "migrate": "node src/scripts/migrate.js",
    "health": "node src/scripts/healthCheck.js",
    "analytics": "node src/scripts/generateAnalytics.js"
  },
  "keywords": ["farm", "marketplace", "api", "nodejs", "mongodb", "express", "advanced"],
  "author": "Farm Marketplace Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.41.0",
    "nodemailer": "^6.9.7",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "redis": "^4.6.10",
    "bull": "^4.12.2",
    "socket.io": "^4.7.4",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "xss": "^1.0.14",
    "hpp": "^0.2.3",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "node-cron": "^3.0.3",
    "sharp": "^0.32.6",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.3",
    "geolib": "^3.3.4",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "validator": "^13.11.0",
    "uuid": "^9.0.1",
    "archiver": "^6.0.1",
    "extract-zip": "^2.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "@types/jest": "^29.5.8",
    "mongodb-memory-server": "^9.1.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}